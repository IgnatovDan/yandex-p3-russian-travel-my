    "tests-testcafe-screenshots-1280": "npx testcafe \"chrome:headless:emulation:width=1280;height=1440;mobile=true;orientation=vertical\" tests-testcafe/1280/**",
    "tests-testcafe-screenshots-1440": "npx testcafe \"chrome:headless:emulation:width=1440;height=1600;mobile=true;orientation=vertical\" tests-testcafe/1440/**",
