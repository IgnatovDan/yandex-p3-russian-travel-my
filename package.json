    "tests-testcafe-screenshots-768": "npx testcafe \"chrome:headless:emulation:width=768;height=1000;mobile=true;orientation=vertical\" tests-testcafe/768/**",
    "tests-testcafe-screenshots-900": "npx testcafe \"chrome:headless:emulation:width=900;height=1000;mobile=true;orientation=vertical\" tests-testcafe/900/**",
    "tests-testcafe-screenshots-1024": "npx testcafe \"chrome:headless:emulation:width=1024;height=1440;mobile=true;orientation=vertical\" tests-testcafe/1024/**",
